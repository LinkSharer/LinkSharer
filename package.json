		"@sveltejs/kit": "^2.12.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/eslint": "^9.6.0",
		"@types/node": "^22.10.5",
		"@types/yargs": "^17.0.33",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"only-allow": "^1.2.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.8",
		"svelte": "^5.15.0",
		"svelte-check": "^4.1.0",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.19.1",
		"vite": "^5.4.10",
		"yargs": "^17.7.2"
	},
	"pnpm": {
